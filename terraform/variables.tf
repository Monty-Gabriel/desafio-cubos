variable "db_user" {
  description = "Database user"
  type        = string
  default     = "myuser"
}

variable "db_password" {
  description = "Database password"
  type        = string
  default     = "mypassword"
}

variable "db_name" {
  description = "Database name"
  type        = string
  default     = "mydb"
}
